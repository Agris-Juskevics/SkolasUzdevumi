#include <iostream>
using namespace ::std;

int main()
{
	int num;
	int divider = 2;

	cout << "Ievadi skaitli:";
	cin >> num;

	while (divider < num) {
		if (num % divider == 0) {
			cout << "Skaitlis nav pirmskaitlis";
			return 0;
		}
		else if (num % divider != 0) {
			divider++;
		}

	}
	cout << "Skaitlis ir pirmskaitlis";
}
